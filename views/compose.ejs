<%- include('partials/header'); -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" placeholder="Write title here..." autocomplete="off">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="3" cols="30" autocomplete='off' placeholder="Write about post here..."></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include('partials/footer'); -%>